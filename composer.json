{
  "name": "manu/store-api",
  "description": "API REST Crud operation on store object",
  "keywords": [
    "php",
    "slim-micro-framework",
    "rest-api",
    "mysql",
    "slim4",
    "slim",
    "rest",
    "api",
    "doctrine",
    "psr7"
  ],
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel DUVERNON",
      "email": "manuel.duvernon@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "doctrine/orm": "^2.8",
    "easybook/slugger": "1.*",
    "monolog/monolog": "^2.2",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.3",
    "slim/slim": "^4.7",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "jangregor/phpstan-prophecy": "^0.8.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.70",
    "phpunit/phpunit": "^8.5",
    "symfony/var-dumper": "^5.2"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "": "src/",
      "App\\": "src/App"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit"
  }
}
